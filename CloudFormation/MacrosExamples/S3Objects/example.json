{
    "Transform": "S3Objects",
    "Resources": {
        "Bucket": {
            "Type": "AWS::S3::Bucket"
        },
        "Object1": {
            "Type": "AWS::S3::Object",
            "DependsOn": "Bucket",
            "Properties": {
                "Target": {
                    "Bucket": {
                        "Ref": "Bucket"
                    },
                    "Key": "README.md",
                    "ContentType": "text/markdown"
                },
                "Body": "# My text file\n\nThis is my text file;\nthere are many like it,\nbut this one is mine.\n"
            }
        },
        "Object2": {
            "Type": "AWS::S3::Object",
            "DependsOn": "Bucket",
            "Properties": {
                "Target": {
                    "Bucket": {
                        "Ref": "Bucket"
                    },
                    "Key": "1-pixel.gif",
                    "ContentType": "image/png"
                },
                "Base64Body": "R0lGODdhAQABAIABAP///0qIbCwAAAAAAQABAAACAkQBADs="
            }
        },
        "Object3": {
            "Type": "AWS::S3::Object",
            "DependsOn": "Bucket",
            "Properties": {
                "Source": {
                    "Bucket": {
                        "Fn::GetAtt": [
                            "Object1",
                            "Bucket"
                        ]
                    },
                    "Key": {
                        "Fn::GetAtt": [
                            "Object1",
                            "Key"
                        ]
                    }
                },
                "Target": {
                    "Bucket": {
                        "Ref": "Bucket"
                    },
                    "Key": "README-copy.md"
                }
            }
        },
        "Object4": {
            "Type": "AWS::S3::Object",
            "DependsOn": "Bucket",
            "Properties": {
                "Target": {
                    "Bucket": {
                        "Ref": "Bucket"
                    },
                    "Key": "readme.md"
                },
                "URL": "https://raw.githubusercontent.com/aws-cloudformation/aws-cloudformation-templates/main/README.md"
            }
        }
    }
}
